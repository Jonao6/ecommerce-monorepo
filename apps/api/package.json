{
  "name": "api",
  "version": "1.0.0",
  "main": "dist/server/index.js",
  "type": "module",
  "files": [
    "dist/",
    "prisma/schema.prisma",
    "prisma/migrations/"
  ],
  "scripts": {
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./prisma/schema.prisma",
    "prisma:seed": "tsx src/lib/seed.ts",
    "prebuild": "pnpm prisma:generate",
    "build": "tsc",
    "start": "node dist/server/index.js",
    "dev": "nodemon --exec \"tsx\" src/server/index.ts"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.1.2",
    "@graphql-tools/merge": "^9.1.1",
    "@graphql-tools/utils": "^10.9.1",
    "@prisma/client": "^6.16.2",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^9.0.2",
    "redis": "^5.8.2",
    "stripe": "^18.5.0",
    "ts-node": "^10.9.2",
    "tsc-esm-fix": "^3.1.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "catalog:prisma",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsc-watch": "^7.1.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
