{
  "name": "api",
  "version": "1.0.0",
  "main": "dist/server/index.js",
  "type": "module",
  "files": [
    "dist/",
    "prisma/schema.prisma",
    "prisma/migrations/"
  ],
  "scripts": {
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./prisma/schema.prisma",
    "prisma:seed": "tsx src/lib/seed.ts",
    "prebuild": "pnpm prisma:generate",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && cp -r src/graphql/schema/*.graphql dist/src/graphql/schema/",
    "start": "node dist/src/server/index.js",
    "dev": "nodemon --exec \"tsx --env-file=.env\" src/server/index.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "generate": "graphql-codegen"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.1.2",
    "@graphql-tools/merge": "^9.1.1",
    "@graphql-tools/utils": "^10.9.1",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "@types/redis": "^4.0.10",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "cloudinary": "^2.7.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "redis": "^5.8.2",
    "stripe": "^18.5.0",
    "ts-node": "^10.9.2",
    "tsc-esm-fix": "^3.1.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/typed-document-node": "^6.1.4",
    "@graphql-codegen/typescript": "^5.0.6",
    "@graphql-codegen/typescript-operations": "^5.0.6",
    "@graphql-codegen/typescript-resolvers": "^5.1.4",
    "@prisma/config": "^7.1.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^7.1.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsc-watch": "^7.1.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
